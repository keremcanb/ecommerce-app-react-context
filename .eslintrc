{
  "extends": ["wesbos"],
  "rules": {
    "no-nested-ternary": 0,
    "react/jsx-props-no-spreading": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "no-unreachable": 0,
    "react/prop-types": 0,
    "arrow-body-style": 0,
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2
      }
    ]
  }
}
